sudo /opt/bitnami/apache2/bin/htpasswd /opt/bitnami/repositories/users <UserName>

sudo svnadmin create /opt/bitnami/repositories/<Repository Name>

sudo chmod 777 -R /opt/bitnami/repositories/<Repository Name>


